From 9e66b1510a47170d2e96b7fe5dcf968b741fb3f1 Mon Sep 17 00:00:00 2001
From: Albert Dahan <a.dahan@metrological.com>
Date: Sun, 28 Jan 2018 21:01:24 +0100
Subject: [PATCH] [WebKitBrowser] Properly set secure flag for UX

---
 WebKitBrowser/UX.config | 1 +
 1 file changed, 1 insertion(+)

diff --git a/WebKitBrowser/UX.config b/WebKitBrowser/UX.config
index 0f0d855..4b1a309 100644
--- a/WebKitBrowser/UX.config
+++ b/WebKitBrowser/UX.config
@@ -17,6 +17,7 @@ map()
     kv(diskcache ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_DISKCACHE})
     kv(xhrcache ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_XHRCACHE})
     kv(noncompositedwebgl true)
+    kv(secure false)
     if(WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_CLIENTIDENTIFIER)
         kv(clientidentifier ${WPEFRAMEWORK_PLUGIN_WEBKITBROWSER_CLIENTIDENTIFIER})
     endif()
